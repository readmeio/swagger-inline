    "start": "babel --watch src/ -d build/",
    "test": "jest --coverage tests/"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "commander": "^6.0.0",
    "globby": "^11.0.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.11",
    "multilang-extract-comments": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@readme/eslint-config": "^5.0.5",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^7.1.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
